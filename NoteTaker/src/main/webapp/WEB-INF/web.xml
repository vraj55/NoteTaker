<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveNote</servlet-name>
  	<display-name>SaveNote</display-name>
  	<description></description>
  	<servlet-class>com.servlets.SaveNote</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteNoteServlet</servlet-name>
  	<display-name>DeleteNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.DeleteNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateNoteServlet</servlet-name>
  	<display-name>UpdateNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.UpdateNoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveNote</servlet-name>
  	<url-pattern>/SaveNote</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteNoteServlet</servlet-name>
  	<url-pattern>/DeleteNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateNoteServlet</servlet-name>
  	<url-pattern>/UpdateNoteServlet</url-pattern>
  </servlet-mapping>
</web-app>
